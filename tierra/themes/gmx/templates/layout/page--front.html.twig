<div id="page-wrapper">
  <div id="page">

    <div id="stick">
      <div class="menu">
        <div class="container">
          <div class="row">
            {% if page.header %}
              <div class="col-xs-6 col-md-4">
                {{ page.header }}
              </div>
            {% endif %}
            <!-- Navigation -->
            {% if page.menu %}
              <div class="col-xs-6 col-md-8">
                {{ page.menu }} 
              </div>
            {% endif %}
            <!--End Navigation -->
          </div>
        </div>
      </div>
    </div>

  <!--Start: Slider -->
    {% if page.slider %}
      <div class="slider">
        {{ page.slider }}
      </div>
    {% endif %}
  <!--End: Slider -->

    {% if page.highlighted %}
        <div class="container">
            <div class="col-md-12">
              {{ page.highlighted }}
            </div>
        </div>
    {% endif %}
 
      <!--- Start Profesionalismo y Eficiencia -->
      {% if page.profesionalismoyeficiencia %}
        <div class="container-fluid profesionalismo-y-eficiencia boxpye">
          <div class="row">
            <div class="col-md-10 col-md-offset-1">
                {{ page.profesionalismoyeficiencia }}
            </div>
          </div>
        </div>
      {% endif %}
      <!---End Profesionalismo y Eficiencia -->

      <!--- Start Qué Hacemos 3 columnas -->
        <div class="container-fluid">
          <div class="row ca">
            <div class="col-md-4">
                {{ page.quea }}
            </div>
            <div class="col-md-4">
                {{ page.queb }}
            </div>
            <div class="col-md-4">
                {{ page.quec }}
            </div>
          </div>
        </div>
      <!---End Qué Hacemos 3 columnas -->

      <!--- Start About -->
      {% if page.about %}
        <div class="container-fluid about box">
          <div class="row">
            <div class="col-md-10 col-md-offset-1">
                {{ page.about }}
            </div>
          </div>
        </div>
      {% endif %}
      <!---End About -->

      <!--- Start Nuestra empresa -->
      {% if page.nuestraempresa %}
        <div class="container-fluid nuestraempresa box">
          <div class="row">
            <div class="col-md-10 col-md-offset-1">
                {{ page.nuestraempresa }}
            </div>
          </div>
        </div>
      {% endif %}
      <!---End Nuestra empresa -->

      <!--- Start Enlaces de interés -->
      {% if page.enlacesdeinteres %}
        <div class="container-fluid enlacesdeinteres">
          <div class="row">
            <div class="col-md-8 col-xs-12 col-md-offset-2">
                {{ page.enlacesdeinteres }}
            </div>
              <div class="col-md-10 col-xs-12 col-md-offset-1">
                <div class="margenenlaces">
                 <div class="col-md-6">{{ page.enlacesdeinteresa }}</div>
                 <div class="col-md-6">{{ page.enlacesdeinteresb }}</div>
                </div>
              </div>
          </div>
        </div>
      {% endif %}
      <!---End Enlaces de interés -->

<!--Start: Social -->
    <div class="container-fluid rs">
      <div class="row">
        <div class="col-md-12">
          <div class="redsocial">
            <div class="sociales">
								<ul class="social">
									<li><a href="#" class="icon fa-facebook"></a></li>
									<li><a href="#" class="icon fa-twitter"></a></li>
									<li><a href="#" class="icon fa-linkedin"></a></li>
								</ul>
              </div>
          </div>
        </div>
      </div>
    </div>
<!--End: Social -->

<!--Start: Footer -->
  <div class="footer">
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          {{ page.footera }}
        </div>
      </div>
    </div>
  </div>
<!--End: Footer -->

  </div>
</div>