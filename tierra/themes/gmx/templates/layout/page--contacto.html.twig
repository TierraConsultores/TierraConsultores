      {% if page.content %}
        <div class="container contenido">
          <div class="row">
            <div class="col-md-12">
              <h1>{{ page['#title'] }}</h1>
            </div>
          </div>
          <div class="row">
            <div class="col-md-7">
              {{ page.content }}
            </div>
            <div class="col-md-5">
              <div class="ContactoTexto">
                {{ page.contacto }}
              </div>
            </div>
          </div>
        </div>
      {% endif %}
      <!---End content -->


