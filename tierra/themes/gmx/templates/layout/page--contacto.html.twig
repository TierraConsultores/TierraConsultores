        {% if page.menu %}
          <div class="col-xs-6 col-md-8">
            {{ page.menu }} 
          </div>
        {% endif %}
        <!--End Navigation -->
      </div>
    </div>
  </div>
</div> 


    {% if page.highlighted %}
        <div class="container">
            <div class="col-md-12">
              {{ page.highlighted }}
            </div>
